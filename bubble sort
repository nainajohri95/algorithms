class Main
{
  static void bsort(int ar[])
    {
      int n = ar.length;
      for(int i=0;i<n-1;i++)
        for(intj=0;j<n-i-1;j++)
            if(ar[j]>ar[j+1])
              {
                int temp = ar[j];
                ar[j] = ar[j+1];
                ar[j+1] = temp;
              }
    }
    public static void main(String args[])
    {
      int data = {-2,45,0,11,-9};
      Main.bsort(data);
      System.out.println(ar.toString(data));
    }
}
